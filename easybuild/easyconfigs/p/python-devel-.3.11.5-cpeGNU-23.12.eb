easyblock = 'PythonBundle'

name = 'python-devel'
version = '.3.11.5'

homepage = 'https://python.org/'
description = """
A bundle containing several common python packages, all with no external dependency,
to be used together with cray-python.

To its nature, it is built on the following modules/bundles:
 * PyPI-bundle/2023.10
 * SciPy-bundle/2023.11
 * GitPython/3.1.42
 * cryptography/41.0.5
 * poetry/1.6.1
 * hatchlin/1.18.0
 * wrapt/1.15.0
 * flit/3.9.0
 * tqdm/4.66.2
 * kaleido/0.2.1
 * archspec/0.2.2
 * scikit-learn/1.4.0
 * PyQt-builder/1.15.4
 * markdown/3.6

NOTE: This module is ONLY to be used as dependency of other modules. If you are
a user, you should use the module miniconda3 and create your own python environment
instead of loading this module.
"""

whatis = [
    "Description: A bundle providing multiple packages for development in cray-python.",
]

toolchain = {'name': 'cpeGNU', 'version': '23.12'}

builddependencies = [
    ('unzip', '6.0', '', SYSTEM),
    ('rust', '1.78.0','', SYSTEM),  # required for cryptography
]
dependencies = [
    ('cray-python/3.11.5', EXTERNAL_MODULE),
    ('wheel','.0.41.2'),
]

# This is necessary to avoid the "No module named '_ctypes'" error
# The libffi path should match that in the above "dependencies"
# See also https://bugs.python.org/issue14527

