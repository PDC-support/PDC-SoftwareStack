-    kstring_t str = {0,0,0};
-
-    htsFile *fp = hts_open(fname,"r");
-    if ( !fp ) goto error;
-
-    while ( hts_getline(fp, KS_SEP_LINE, &str) > 0 )
-    {
-        if ( regidx_insert(idx, str.s) ) goto error;
-    }
-    regidx_insert(idx, NULL);
