-/* As int32_decoded/encode, but from/to blocks instead of cram_fd */
-int int32_get_blk(cram_block *b, int32_t *val) {
-    if (b->uncomp_size - BLOCK_SIZE(b) < 4)
-	return -1;
-
-    *val =
-	 b->data[b->byte  ]        |
-	(b->data[b->byte+1] <<  8) |
-	(b->data[b->byte+2] << 16) |
-	(b->data[b->byte+3] << 24);
-    BLOCK_SIZE(b) += 4;
-    return 4;
-}
