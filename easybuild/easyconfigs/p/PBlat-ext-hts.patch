-    $ln1[5] =~ s/^$/*/g;
-    $ln2[5] =~ s/(\D|^)0\D/$1/g;
-    $ln2[5] =~ s/^$/*/g;
-
-    # Fix 10M10M cigar to 20M
-    $ln1[5] =~ s/(\d+)(\D)(\d+)(\2)/$1+$3.$2/e;
-    $ln2[5] =~ s/(\d+)(\D)(\d+)(\2)/$1+$3.$2/e;
