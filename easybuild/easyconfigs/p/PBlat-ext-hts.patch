-static void free_cache(BGZF *fp)
-{
-    khint_t k;
-    khash_t(cache) *h = (khash_t(cache)*)fp->cache;
-    if (fp->is_write) return;
-    for (k = kh_begin(h); k < kh_end(h); ++k)
-        if (kh_exist(h, k)) free(kh_val(h, k).block);
-    kh_destroy(cache, h);
