-
-    if (hdr->landmark)
-	free(hdr->landmark);
-
-    if (hdr->preservation_map)
-	kh_destroy(map, hdr->preservation_map);
-
-    for (i = 0; i < CRAM_MAP_HASH; i++) {
-	cram_map *m, *m2;
-	for (m = hdr->rec_encoding_map[i]; m; m = m2) {
-	    m2 = m->next;
-	    if (m->codec)
