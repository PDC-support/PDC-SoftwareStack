-int cram_load_reference(cram_fd *fd, char *fn) {
-    int ret = 0;
-
-    if (fn) {
-	fd->refs = refs_load_fai(fd->refs, fn,
-				 !(fd->embed_ref && fd->mode == 'r'));
-	fn = fd->refs ? fd->refs->fn : NULL;
-	if (!fn)
-	    ret = -1;
-	sanitise_SQ_lines(fd);
