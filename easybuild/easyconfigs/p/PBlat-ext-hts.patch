-	else
-	    rd+=4;
-
-	if (crc != c->crc32) {
-	    fprintf(stderr, "Container header CRC32 failure\n");
-	    cram_free_container(c);
-	    return NULL;
-	}
-    }
-
-    c->offset = rd;
-    c->slices = NULL;
-    c->curr_slice = 0;
-    c->max_slice = c->num_landmarks;
-    c->slice_rec = 0;
-    c->curr_rec = 0;
-    c->max_rec = 0;
-
-    if (c->ref_seq_id == -2) {
-	c->multi_seq = 1;
-	fd->multi_seq = 1;
-    }
-
-    fd->empty_container =
-	(c->num_records == 0 &&
-	 c->ref_seq_id == -1 &&
-	 c->ref_seq_start == 0x454f46 /* EOF */) ? 1 : 0;
-
-    return c;
-}
