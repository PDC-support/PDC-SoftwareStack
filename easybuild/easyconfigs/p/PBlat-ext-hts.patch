-	idx_stack[idx_stack_ptr] = idx;
-
-	while (pos < kstr.l && kstr.s[pos] != '\n')
-	    pos++;
-	pos++;
-    } while (pos < kstr.l);
-
-    free(idx_stack);
-    free(kstr.s);
-    free(fn2);
-
-    // dump_index(fd);
-
-    return 0;
-}
