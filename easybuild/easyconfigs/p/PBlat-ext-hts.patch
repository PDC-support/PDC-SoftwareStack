-int cram_block_compression_hdr_decoder2encoder(cram_fd *fd,
-					       cram_block_compression_hdr *ch) {
-    int i;
-
-    if (!ch || !ch->codecs)
-	return -1;
-
-    for (i = 0; i < DS_END; i++) {
-	cram_codec *co = ch->codecs[i];
-	if (!co)
-	    continue;
-
-	if (-1 == cram_codec_decoder2encoder(fd, co))
-	    return -1;
-    }
-
-    return 0;
-}
