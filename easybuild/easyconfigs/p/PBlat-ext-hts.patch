-        my @s1 = split("","@ln1");
-        my @s2 = split("","@ln2");
-        my $ptr = "";
-        for (my $i=0; $i < $#s1; $i++) {
-            if ($s1[$i] eq $s2[$i]) {
-                $ptr .= "-";
-            } else {
-                last;
-            }
-        }
-        print "1\t@ln1\n2\t@ln2\n\t$ptr^\n\n";
-        exit(1);
-    }
-
-    $ln1 = <$fd1>;
-    $ln2 = <$fd2>;
-
-    $c1++; $c2++;
-}
-
-if (defined($ln1)) {
-    print "EOF on $fn1\n";
-    exit(1);
-}
-
-if (defined($ln2)) {
-    print "EOF on $fn2\n";
-    exit(1);
-}
-
-close($fd1);
-close($fd2);
-
-exit(0);
