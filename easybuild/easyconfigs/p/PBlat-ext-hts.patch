-	c->encode = cram_beta_encode_int;
-    else
-	c->encode = cram_beta_encode_char;
-    c->store  = cram_beta_encode_store;
-
-    if (dat) {
-	min_val = ((int *)dat)[0];
-	max_val = ((int *)dat)[1];
-    } else {
-	min_val = INT_MAX;
-	max_val = INT_MIN;
-	int i;
-	for (i = 0; i < MAX_STAT_VAL; i++) {
-	    if (!st->freqs[i])
-		continue;
-	    if (min_val > i)
-		min_val = i;
-	    max_val = i;
-	}
-	if (st->h) {
-	    khint_t k;
-
-	    for (k = kh_begin(st->h); k != kh_end(st->h); k++) {
-		if (!kh_exist(st->h, k))
-		    continue;
-	    
-		i = kh_key(st->h, k);
-		if (min_val > i)
-		    min_val = i;
-		if (max_val < i)
-		    max_val = i;
