-    /* Name */
-    cd = hdr->codecs[DS_RN];
-    bnum1 = cram_codec_to_id(cd, &bnum2);
-    if (bnum1 < 0 && bnum2 >= 0) bnum1 = bnum2;
-    if (cram_ds_unique(hdr, cd, bnum1)) {
-	cram_block *b = cram_get_block_by_id(s, bnum1);
-	if (b) *name_size = b->uncomp_size;
