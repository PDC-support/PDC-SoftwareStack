-    }
-
-    if (memcmp(def->magic, "CRAM", 4) != 0) {
-	free(def);
-	return NULL;
-    }
-
-    if (def->major_version > 3) {
-	fprintf(stderr, "CRAM version number mismatch\n"
-		"Expected 1.x, 2.x or 3.x, got %d.%d\n",
-		def->major_version, def->minor_version);
-	free(def);
-	return NULL;
-    }
-
-    fd->first_container += 26;
-    fd->last_slice = 0;
-
-    return def;
-}
