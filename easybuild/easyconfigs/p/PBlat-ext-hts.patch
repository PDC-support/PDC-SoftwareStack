-	    m2 = m->next;
-	    if (m->codec)
-		m->codec->free(m->codec);
-	    free(m);
-	}
-    }
-
-    for (i = 0; i < DS_END; i++) {
-	if (hdr->codecs[i])
-	    hdr->codecs[i]->free(hdr->codecs[i]);
-    }
-
-    if (hdr->TL)
-	free(hdr->TL);
-    if (hdr->TD_blk)
-	cram_free_block(hdr->TD_blk);
-    if (hdr->TD_hash)
-	kh_destroy(m_s2i, hdr->TD_hash);
