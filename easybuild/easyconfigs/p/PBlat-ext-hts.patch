-	} else {
-	    if (30 != hwrite(fd->fp,
-			     "\x0b\x00\x00\x00\xff\xff\xff\xff"
-			     "\x0f\xe0\x45\x4f\x46\x00\x00\x00"
-			     "\x00\x01\x00\x00\x01\x00\x06\x06"
-			     "\x01\x00\x01\x00\x01\x00", 30))
-		return -1;
-	}
-    }
-
-    for (bl = fd->bl; bl; bl = next) {
-	int i, max_rec = fd->seqs_per_slice * fd->slices_per_container;
-
-	next = bl->next;
-	for (i = 0; i < max_rec; i++) {
-	    if (bl->bams[i])
