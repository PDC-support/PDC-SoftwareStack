-	    free(b->data);
-	    b->data = (unsigned char *)comp;
-	    b->comp_size = comp_size;
-	    b->method = method;
-	}
-
-    } else {
-	// no cached metrics, so just do zlib?
-	comp = cram_compress_by_method((char *)b->data, b->uncomp_size,
-				       &comp_size, GZIP, level, Z_FILTERED);
-	if (!comp) {
-	    fprintf(stderr, "Compression failed!\n");
-	    return -1;
-	}
-	free(b->data);
-	b->data = (unsigned char *)comp;
-	b->comp_size = comp_size;
-	b->method = GZIP;
