-// Destroy a bit set.
-static inline void kbs_destroy(kbitset_t *bs)
-{
-	free(bs);
-}
-
-// Reset the bit set to empty.
-static inline void kbs_clear(kbitset_t *bs)
-{
-	memset(bs->b, 0, bs->n * sizeof (unsigned long));
